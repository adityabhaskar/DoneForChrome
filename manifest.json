{
  "version": "0.0.1.1",
  "version_name": "0.0.1.1 pre-alpha iDoneThis API",
  "name": "__MSG_appName__",
  "short_name": "__MSG_shortName__",
  "description": "__MSG_appDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/idonethis.js",
      "js/background.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Comma",
        "mac": "Command+Comma"
      }
    }
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon-19.png",
      "38": "img/icon-38.png"
    },
    "default_title": "__MSG_browserButtonTitle__",
    "default_popup": "popup.html"
  },
  "icons": {
      "16": "img/icon-16.png",
      "128": "img/icon-128.png"
  },
  "options_page": "options.html",
  "omnibox": {
    "keyword": "done"
  },
  "permissions": [
    "notifications",
    "storage",
    "https://idonethis.com/api/*"
  ]
}
