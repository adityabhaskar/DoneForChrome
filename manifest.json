{
  "version": "0.0.2.0",
  "version_name": "0.0.2.0 beta",
  "name": "__MSG_appName__",
  "short_name": "__MSG_shortName__",
  "description": "__MSG_appDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/idonethis.js",
      "js/background.js"
    ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "mac": "Command+Shift+Z",
        "windows": "Ctrl+Shift+Z",
        "chromeos": "Ctrl+Shift+Z",
        "linux": "Ctrl+Shift+Z"
      }
    }
  },
  "browser_action": {
    "default_icon": {
      "19": "img/done-19.png",
      "38": "img/done-38.png"
    },
    "default_title": "__MSG_browserButtonTitle__",
    "default_popup": "popup.html"
  },
  "icons": {
      "16": "img/done-16.png",
      "32": "img/done-32.png",
      "48": "img/done-48.png",
      "128": "img/done-128.png",
      "256": "img/done-256.png",
      "512": "img/done-512.png"
  },
  "options_page": "options.html",
  "omnibox": {
    "keyword": "done"
  },
  "permissions": [
    "notifications",
    "storage",
    "https://idonethis.com/api/*",
    "alarms"
  ]
}
